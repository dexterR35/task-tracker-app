{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",

  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/maintenance.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

