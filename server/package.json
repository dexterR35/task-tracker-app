{
  "name": "task-tracker-server",
  "version": "1.0.0",
  "description": "PERN backend - PostgreSQL, Express, auth API",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "db:schema": "psql -d task_tracker -f db/schema.sql",
    "db:seed": "psql -d task_tracker -f db/seed-user.sql",
    "db:reset": "npm run db:schema && npm run db:seed"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "socket.io": "^4.8.3"
  }
}
